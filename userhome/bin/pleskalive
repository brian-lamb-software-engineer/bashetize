#!/bin/bash
# checks if plesk servers are alive.  Is in order, e.g. 1,2,3,4,5
echo "... plesk1"
curl -I https://uefi.org | grep 200; 
echo "... plesk2"
curl -I http://pcisig.com | grep 200;
echo "... plesk3"
curl -I https://cms.openconnectivity.org | grep 200;
echo "... plesk4"
curl -I https://digital-cp.com | grep 200;
echo "... plesk5"
curl -I https://sata-io.org | grep 200;
