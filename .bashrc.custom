#.bashrc.custom
#
# To bashitize, include the follwing two global files in ~/etc
if [ -f $HOME/etc/.bash-rc-global ]; then
    . $HOME/etc/.bash-rc-global
fi
if [ -f $HOME/etc/.bash-profile-global ]; then
    . $HOME/etc/.bash-profile-global
fi

